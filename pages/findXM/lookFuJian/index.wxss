.file_warn {
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  color: #ec4141;
  padding-left: 30rpx;
}

.imags {
  background-color: #fff;
}

.image_item {
  width: 100%;
  height: 100%;
  text-align: center;
  padding: 10rpx;
}

.image_item image {
  display: block;
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto;
  background-color: #eee;
  border-radius: 10rpx;
}

.image_item text {
  font-size: 26rpx;
  color: #666;
}
