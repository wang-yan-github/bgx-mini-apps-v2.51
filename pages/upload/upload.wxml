<!--pages/upload/upload.wxml-->
<!-- 上传图片 -->
<wux-upload count='{{countNumber}}' list-type="picture-card" file-list="{{ fileList }}" url="{{fileUrl}}" formData='{{formData}}' bind:change="onChange" bind:success="onSuccess" bind:fail="onFail" bind:complete="onComplete" bind:preview="onPreview" bind:remove='onRemove'>
  <text style='font-size:30rpx;color:#666;'>上传</text>
</wux-upload>
